{
  "manifest_version": 2,
  "name": "recolor",
  "version": "0.1",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage",
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["config.js", "recolor.js"]
    }
  ]
}
